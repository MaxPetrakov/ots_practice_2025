from turtle import*
tracer(0)
left(90)
k = 20
for i in range(2):
    forward(10*k)
    left(90)
    forward(5*k)
    left(90)
    forward(10*k)
    right(90)
    forward(5*k)
    right(90)
forward(10*k)
up()
##for x in range(-30,30):
